	text-align: left;
}
/*
@about:商品列表头部样式 右侧;
*/
.listViewHead .listViewHeadRight {
	text-align: right;
}
/*
@about:商品列表头部样式 装饰的|;
*/
.decoration {
	border-left:4px solid #f00;
}
/*
@about:一行内商品列表内容样式;
*/
