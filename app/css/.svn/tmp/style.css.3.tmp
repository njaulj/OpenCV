	background-color: #fff;
	height: 148px;
	padding:10px;
}
/*
@about:一行内商品单品样式;
*/
.lineListViewItem {
	height:100%;
	width:100px;
	background-color: #b2b2b2;
	margin-right:12px;
	display: inline-block;
	max-width: 100px;
	min-width: 100px;	
}
/*
@about:一行内商品单品图片样式;
*/
.lineImg {
	height:100px;
	width:100px;
	background-color: #ED5F6D;
}

.lineImg img {
	width: 100%;
	height: auto;
}
/*
@about:一行内商品单品价格样式;
*/
.linePrice {
	position: absolute;
	top:88px;
	width: 100px;
	height: 1em;
	background-color: #000;
	opacity: 0.3;
	font-size: 1em;
	color:#fff;
	text-align: right;
	line-height: 1em;
}

.linePrice:first-letter {
	font-size: 0.8em;
}
/*
@about:一行内商品单品标题样式;
*/
.lineTitle {
	text-align: center;
	padding:4px 0;
}

/*
@about:单行商品整体样式;
*/
.lightListView {
	background: #fff;
	margin-bottom: 10px;
}
.lightListViewContent {
	padding:10px;
	background-color: #fff;
}

/*
@about:单行商品单品样式;
*/
.lightListViewItem {
	width: 100%;
	background-color: #fff;
	border-bottom: solid 1px #f2f2f2;
	padding: 0.66em 0;
}

/*
@about:单行商品的图片样式;
*/
.lightImg {
	min-width: 100px;
	max-width:132px;
	min-height: 70px;
	max-height: 92px;
	height:auto;
	margin-right:10px;
}

.lightImg img {
	width: 100%;
	height: auto;
}

/*
@about:单行商品的信息样式;
*/
.lightInfo {
	position: relative;
	padding-right: 10px;
}

/*
@about:单行商品的标题样式;
*/
.lightTitle{
	color:#a2a2a2;
	font-weight: 500;
	line-height: 14px;
	display: -webkit-box;
	display: -moz-box;
	margin:0 0 8px 0;
	word-break:break-all;
	-webkit-line-clamp:1;
	-moz-line-clamp:1;
	-webkit-box-orient: vertical;
	overflow:hidden;
	font-size: 14px;
	height:14px;
}

/*
@about:单上商品价格样式;
*/
.lightPrice{
	vertical-align:middle;
	margin:auto 0;
	line-height: 20px;
	height:auto;
}

/*
@about:单品商品价格 现价;
*/
.lightPrice .nowPrice {
	color:#f00;
	font-weight: 500;
	display: inline-block;
}

/*
@about:单品商品价格 现价第一个字母略小;
*/
.lightPrice .nowPrice:first-letter{
	font-size: 80%;
}	

/*
@about:单品商品价格 原价;
*/
.lightPrice .originPrice {
	color:#b2b2b2;
	font-size: 80%;
	text-decoration: line-through;
	display: inline-block;
}

/*
@about:单品商品其余信息;
*/
.lightInfoElse{
	position: relative;
	bottom: 0;
	line-height: 18px;
	font-size: 80%;
	color:#b2b2b2;
	margin-top:8px;
	padding-right: 10px;
	width: 100%;
}

