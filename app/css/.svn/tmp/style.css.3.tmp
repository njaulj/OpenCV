.user-item.under-line{border-bottom: 1px solid #efefef;}
.user-item>*{display: inline-block; font-size: 1.2em;}
.user-item>.content{padding: 0px 5px; margin-bottom: 0px;}
.user-item>.nav{float: right; color: #bcbdbf;}
.user-item table{width: 100%;}
.user-item table td{text-align: center; font-size: 12px;}
.user-setting .list>.item{border-color: #f6f6f6}
.setting-icon{background: url("/img/setting-icon.png") no-repeat; width: 35px; height: 35px; display: inline-block; margin-top: 10px; background-size: cover; background-color: none;}
.setting-icon.order-icon{background-position: 0px 0px; height: 35px;}
.setting-icon.receive-money{background-position: 0px 0px; height: 35px;}
.setting-icon.receive-goods{background-position: 0px -35px; height: 35px;}
.setting-icon.exchange-goods{background-position: 0px -70px; height: 35px;}
.setting-icon.change-passwords{background-position: 0px -140px; height: 35px;}
.setting-icon.change-paywords{background-position: 0px -175px; height: 35px;}
.setting-icon.my-cards{background-position: 0px -210px; height: 35px;}
.setting-icon.my-messages{background-position: 0px -245px; height: 35px;}
.setting-icon.about-us{background-position: 0px -280px; height: 35px;}

.password-list>.item{vertical-align: middle; line-height: 30px; padding: 10px 16px; margin-bottom: 10px;}
.password-list>.item .input-clear{background: none;}
.password-list .next-button{width: 100%; text-align: center; margin: 0 auto;}
.password-list .next-button>button.button{width: 80%; color: #999; padding: 8px; background-color: #fff;}

.contact-list .item{border: none; margin-bottom: 15px;}
.contact-list .item-nav{height: 50px; padding: 0px; border: none;}
.contact-list .item .tab-item{max-width: none;}
.contact-list .item .tab-item.active{border-bottom: 2px solid #e10017;}
.contact-list .item-clear{border: none; background: none; margin: 5px 0px 0px; padding: 5px 16px;}
.contact-list .input-clear{background: none;}
.contact-list .item .remark{padding: 0px 7px; width: 100%;}
.contact-list .submit{width: 80%; position: relative; text-align: center; margin: 0 auto; padding-top: 10px;}

.help-list .item{border-color: #f0f0f0;}
.help-list .item>div{display: inline-block; float: left;}
.help-list .item>.left-item{width: 30%;}
.help-list .item>.left-item>*{display: block; font-size: 1em; text-align: center;}
.help-list .item>.left-item>img{text-align: center; margin: 0 auto 5px auto;}
.help-list .item>.right-item{width: 35%;border-left: 1px solid #f0f0f0;}
.help-list .item>.right-item.no-border{border: none;}
.help-list .item>.right-item>*{display: block; text-align: center;padding: 8px 0px;}
.help-list .item>.right-item>*:first-child{ border-bottom: 1px solid #f0f0f0;}
/*===================分类====================*/
.title-words *,.title-words{font-size: 14px;}
.content-words *,.content-words,.content-words p,.content-words h3{font-size: 12px;}
.title-words .bar .button{font-size: 12px;}

.goods-category {}
.goods-category .item .price1{color: #e10017; font-weight: bold;}
.goods-category .item .price2{text-decoration: line-through;}
.goods-category .item .bottom{position: absolute;bottom: 8px;}

.goods-row{background-color: #f9f9f9; font-size: 12px;}
.category-list>a.item{border: none; background-color: #f9f9f9;}
.category-list>a.item.active{background-color: #fff; color: #e30c46; border-left: 5px solid #e30c46; margin-right: -3px;}
.goods-row .col-25{padding: 2px 2px;}
.goods-row .col-75{background-color: #f9f9f9; padding: 0px;border-left: 1px solid #f3f3f3;}
.category-row{background-color: #fff;}
.category-row>div.col{text-align: center}
.category-row>div.col>.button{max-height: 200px; max-width: 100%;}
.category-row>div.col>.button img{max-width: 100%; display: block;}

.goods-detail{width: 100%; background-color: #eeeeee;}
.goods-detail .item{width: 100%; padding: 0px; border: none; margin-bottom: 15px;min-height: 50px;}
.goods-detail .item.no-background{background: none;}
.goods-detail .item.no-margin{margin-bottom: 0px;}
.goods-detail .item>.slider-img{width: 100%; max-height: 300px; position: relative; height: 300px; background-color: #ececec;}
.goods-detail .item>.slider-img img{width: 100%; max-height: 300px; height: 300px;}
.goods-detail .item>.content{padding: 15px; position: relative; box-sizing: border-box;}
.goods-detail .item>.content.align-center{text-align: center;}
.goods-detail .item>.content .pull-right{float: right;}
.goods-detail .item>.content .line-v{height: 1px; background-color: #dddddd; position: absolute; top: 50%; left: 15px; right: 15px; z-index: -1}

.goods-detail .row.align-center{text-align: center;}
.goods-detail .item .goods-info-nav>.active{color: #ef5f6b; border-bottom: 2px solid #ef5f6b;}
.goods-detail .item .goods-price{color: #ff5000; font-size: 1.3em;height: 40px; padding: 10px 0px;}
.line-through{text-decoration: line-through;}
.goods-detail-footer{padding: 0px; border-top: 1px solid #d8d8d8; height: 55px;}
.goods-detail-footer .row.align-center{text-align: center;}
.goods-detail-footer .row .col{border-left: 1px solid #d8d8d8; background-color: #efefef; line-height: 55px; vertical-align: middle; padding-top: 8px;}
.goods-detail-footer .row .col.add-package{background-color: #ef5f6b; color: #fff; padding-top: 0px;}
.goods-detail-footer .row .col:first-child{border-left: none;}
.goods-detail-footer .row .col>*{line-height: 25px; display: block;}
.goods-detail-footer *{padding: 0px;}

.select-pane{position: absolute; width: 100%;z-index: 99; top: 0px; right: 0px; bottom: 0px; display: none;}
.pop-pane>.bg{width: 100%; height: 100%; background-color: #999; opacity: 0.8; position: absolute; left: 0px; top: 0px; z-index: 99;}
.select-pane>.content{width: 80%; height: 100%;z-index: 100; position: absolute; right: 0px; top: 0px; background-color: #fff;}
.select-pane .type-name{text-align: center; margin-top: 10px;}
.select-pane .type-name.no-margin{margin-top: 0px;}
.select-pane .type-item{border: 1px solid #c0c0c0; border-radius: 5px; color: #5b5a5a; text-decoration: none; padding: 5px 20px;display: inline-block; float: left;
	margin: 5px 0px;}
.select-pane .type-item.active{border-color: #f15353;}

.button-group{border: 1px solid #cccccc; border-radius: 5px; display: inline-block; overflow: hidden;}
.button-group>*{padding: 5px 15px; display: inline-block;}
.button-group>.button-content{border: 1px solid #c0c0c0; border-top: none; border-bottom: none;}

.select-pane .bottom{}

.charact-type,.charact-filter{width: 100%; position: absolute; top: 90px; bottom: 49px; z-index: 99; display: none;}
.charact-type>.content{width: 100%; min-height: 80px;z-index: 100; position: relative;}
.charact-type>.content>.bg{width: 100%; height: 100%; background-color: #fff; opacity: 0.9; position: absolute; left: 0px; top: 0px; z-index: 99;}
.charact-type>.content>.list{padding: 5px 10px;}
.charact-type>.content>.list *{z-index: 100;}
.charact-type>.content>.list>a.item{border:none; background: none; padding: 5px 10px;}
.charact-type>.content>.list>a.item.active{color: #e95252;}
.icon-32{font-size: 28px;}
.row>.col.align-right{text-align: right;}
.row>.col.align-center{text-align: center;}

.charact-filter>.content{width: 80%; height: 100%; background-color: #f3f5f7;z-index: 999; position: absolute; right: 0px; top: 0px;}
.charact-filter .buttons{text-align: center; padding: 10px;}
.charact-filter .buttons>*{margin: 0px 5px; padding: 1px 9px; background-color: #fff;}
.charact-filter .buttons>*>i{display: none;}
.charact-filter .buttons>.activated{border-color: #f15353; color: #f15353; background-color: #fff;}
.charact-filter .buttons>.active{border-color: #f15353; color: #f15353; background-color: #fff;}
.charact-filter .buttons>.active>i{display: inline-block;}