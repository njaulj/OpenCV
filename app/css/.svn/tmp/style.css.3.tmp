.clear{ clear:both} 
/*
@about:padding:0;
*/
.padZero {
	padding:0;
}

/*
@about:headbar 颜色
*/
.back-text {
  display: none;
}
.headbar {
  background-color: #ED5F6D;
  color: #fff;
  font-weight: 500;
  border-bottom: none;
}
.charactBars {
  color: #666;
  background-color: #fff;
}
.charactBars .normalCharactBar {
  display: inline-block;
  margin: 8px 8px 0 8px;
  padding-bottom: 4px;
}
.charactBars .activeCharactBar {
  display: inline-block;
  margin: 8px 8px 0 8px;
  padding-bottom: 4px;
  color: #ED5F6D;
  border-bottom: solid 4px #ED5F6D;
}
.charactBars .navCharactBlock {
  position: absolute;
  top: 37px;
  left: 0;
  z-index: -1;
  width: 100%;
  opacity: 0.9;
}
.charactBars .navCharactBlock .navCharactBtn {
  padding: 8px 8px;
  border: solid 1px #f2f2f2;
  color: #333;
  background-color: #fff;
}
.charactBars .navCharactBlock .active-navCharactBtn {
  padding: 8px 8px;
  border-right: solid 1px #f2f2f2;
  border-bottom: solid 1px #f2f2f2;
  color: #ED5F6D;
  background-color: #fff;
}
.arrowDown {
  position: absolute;
  top: 0;
  right: 0;
  width: 40px;
  height: 36px;
  border-left: solid 1px #f2f2f2;
  background-color: #fff;
  min-width: 40px;
  min-height: 37px;
  font-size: 1.6em bold;
  color: #a2a2a2;
}





/*
@about:首页 头上的headBanner 的高度;
*/
.headBanner{
	height: 150px;
	width:100%;
	background-color: #f2f2f2;
}

/*
@about:幻灯片的页脚 放置右下角;
*/
.headBanner .slider-pager {
    position: absolute;
    bottom: 10px;
    z-index: 1;
    width: 100%;
    height: 15px;
    text-align: right;
}

/*
@about:幻灯片页脚颜色改成白色;
*/
.slider-pager .slider-pager-page {
    display: inline-block;
    margin: 0px 5px;
    width: 8px;
    color: #fff;
    text-decoration: none;
    opacity: 0.3; 
    font-size:0.5em;
}

.headBanner img {
	width: 100%;
	height:100%;
}

/*
@about:首页四个NavBtn 设置居中;
*/
.navBtns {
	background-color: #fff;
}
.navRow {
	padding:14px 0;
}

.navCol {
	text-align: center;
}
/*
@about:首页四个NavBtn的图片显示大小;
*/
.navImg {
	display: block;
	width:100%;
	height:auto;
	margin: 0 auto;
	padding:0 14px;

}

.navTitle {
	display: block;
	height:24px;
	padding-top:8px;
}
/*
@about:首页广告区域;
*/
.ads {
	border: 1px solid #f2f2f2;
	background-color: #fff;
}
/*
@about:左侧广告;
*/
.leftad {
	border-right:1px solid #f2f2f2;
	height:131px;
}
/*
@about:右侧广告;
*/
.rightad{
	height:65px;
}
/*
@about:右侧广告 奇数带下边框;
*/
.rightad:nth-child(odd){
	border-bottom: 1px solid #f2f2f2;
}
/*
@about:中间的Banner 样式;
*/
.centerBanner {
	padding:10px;
	background-color: #f2f2f2;
}
/*
@abut:中间的banner Item的样式;
*/
.centerBannerItem {
	background-color: #fff;
	border-right:solid 1px #f2f2f2;
	height:150px;
}

/*
@about:一行内商品列表 整体样式;
*/
.lineListView {
	width: 100%;
	padding-bottom:10px;
}

/*
@about:商品列表头部样式;
*/
.listViewHead {
	padding:10px;
	background-color: #fff;
	font-size:12px;
}
/*
@about:商品列表头部样式 左侧 ;
*/
