<ion-view>
  <ion-header-bar class="bar headbar">
    <label class="title">感兴趣的商品</label>
  </ion-header-bar>
  <ion-header-bar class="bar-subheader">
    <div class="row  padZero">
      <div class="col col-50 padZero favorTabDiv">
        <button ng-click="chooseTab(0)" ng-class="chooseTabIndex?'button normalTabBtn':'button activeTabBtn'">我收藏的商品</button>
      </div>
      <div class="col col-50 padZero favorTabDiv">
        <button ng-click="chooseTab(1)" ng-class="chooseTabIndex?'button activeTabBtn':'button normalTabBtn'">浏览过的商品</button>
      </div>
    </div>
  </ion-header-bar>
  <ion-content style="font-size:71.4%">
    <ion-tabs delegate-handle="favorTabDelegate" class="tabs-light favorPage" style="border-bottom:solid 1px #f2f2f2">
      <ion-tab title="我收藏的商品">
        <ion-nav-view name="collect-tab">
          <!-- 沿用首页定义的商品列表2 -->
          <div class="lightListView">
            <div class="lightListViewContent">
              <div class="lightListViewItemOne">
